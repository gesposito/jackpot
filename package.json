{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "feed": "node ./feed.js",
    "start": "node ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "seed:all": "node_modules/.bin/sequelize db:seed:all",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.2.0",
    "express": "^4.14.0",
    "lodash": "^4.16.6",
    "morgan": "^1.7.0",
    "mysql": "^2.12.0",
    "request": "^2.76.0",
    "request-promise-native": "^1.0.3",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "supertest": "^2.0.0"
  }
}
